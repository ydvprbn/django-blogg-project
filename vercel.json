{
  "version": 2,
  "builds": [
    {
      "src": "django_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "django_project/wsgi.py"
    }
  ]
}